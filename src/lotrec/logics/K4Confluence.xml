<?xml version="1.0" encoding="UTF-8"?>
<logic>
    <parser-version>2.1</parser-version>
    <description>K4 + Confluence. Implement the optimized algorithm of Gasquet and Farinas 1999.
Author: Lilac Team
Last update: June 2007</description>
    <connector>
        <connector-name>and</connector-name>
        <arity>2</arity>
        <associative>true</associative>
        <output-format>_ &amp; _</output-format>
        <priority>3</priority>
        <connector-comment/>
    </connector>
    <connector>
        <connector-name>not</connector-name>
        <arity>1</arity>
        <associative>false</associative>
        <output-format>~_</output-format>
        <priority>5</priority>
        <connector-comment/>
    </connector>
    <connector>
        <connector-name>or</connector-name>
        <arity>2</arity>
        <associative>true</associative>
        <output-format>_ V _</output-format>
        <priority>3</priority>
        <connector-comment/>
    </connector>
    <connector>
        <connector-name>nec</connector-name>
        <arity>1</arity>
        <associative>false</associative>
        <output-format>[]_</output-format>
        <priority>4</priority>
        <connector-comment/>
    </connector>
    <connector>
        <connector-name>pos</connector-name>
        <arity>1</arity>
        <associative>false</associative>
        <output-format>&lt;&gt;_</output-format>
        <priority>4</priority>
        <connector-comment/>
    </connector>
    <rule>
        <rule-name>Stop</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>not variable A</parameter>
        </condition>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>variable A</parameter>
        </condition>
        <action>
            <action-name>stop</action-name>
            <parameter>u</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>FALSE</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>And</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>and variable A variable B</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>variable A</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>variable B</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>NotAnd</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>not and variable A variable B</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>or not variable A not variable B</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>NotOr</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>not or variable A variable B</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>and not variable A not variable B</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>NotNot</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>not not variable A</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>variable A</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>Or</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>or variable A variable B</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>duplicate</action-name>
            <parameter>premodel_copy</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>not variable A</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>premodel_copy.u</parameter>
            <parameter>not variable B</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>NotNec</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>not nec variable A</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>pos not variable A</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>NotPos</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>not pos variable A</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>nec not variable A</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>createSuccessor</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>pos variable A</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <action>
            <action-name>createNewNode</action-name>
            <parameter>u</parameter>
        </action>
        <action>
            <action-name>link</action-name>
            <parameter>u</parameter>
            <parameter>u</parameter>
            <parameter>R</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>variable A</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>loopTest</rule-name>
        <condition>
            <condition-name>isNewNode</condition-name>
            <parameter>u</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Waiting</parameter>
        </condition>
        <condition>
            <condition-name>isAncestor</condition-name>
            <parameter>u</parameter>
            <parameter>u</parameter>
        </condition>
        <condition>
            <condition-name>contains</condition-name>
            <parameter>u</parameter>
            <parameter>u</parameter>
        </condition>
        <action>
            <action-name>mark</action-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>propagateNec</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>nec variable A</parameter>
        </condition>
        <condition>
            <condition-name>isLinked</condition-name>
            <parameter>u</parameter>
            <parameter>u</parameter>
            <parameter>R</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Waiting</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>nec variable A</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>variable A</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>createNewRoot</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>ROOT</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Waiting</parameter>
        </condition>
        <action>
            <action-name>createNewNode</action-name>
            <parameter>u</parameter>
        </action>
        <action>
            <action-name>mark</action-name>
            <parameter>u</parameter>
            <parameter>Waiting</parameter>
        </action>
        <action>
            <action-name>mark</action-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>ROOT</parameter>
        </action>
        <action>
            <action-name>link</action-name>
            <parameter>u</parameter>
            <parameter>u</parameter>
            <parameter>R</parameter>
        </action>
        <rule-comment/>
    </rule>
    <rule>
        <rule-name>confluence</rule-name>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>nec variable A</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <condition>
            <condition-name>isMarked</condition-name>
            <parameter>u</parameter>
            <parameter>CONTAINED</parameter>
        </condition>
        <condition>
            <condition-name>isAncestor</condition-name>
            <parameter>node2</parameter>
            <parameter>u</parameter>
        </condition>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>node2</parameter>
            <parameter>ROOT</parameter>
        </condition>
        <condition>
            <condition-name>isAncestor</condition-name>
            <parameter>node2</parameter>
            <parameter>u</parameter>
        </condition>
        <condition>
            <condition-name>hasElement</condition-name>
            <parameter>u</parameter>
            <parameter>ROOT</parameter>
        </condition>
        <condition>
            <condition-name>isNotMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </condition>
        <condition>
            <condition-name>isMarked</condition-name>
            <parameter>u</parameter>
            <parameter>Waiting</parameter>
        </condition>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>nec variable A</parameter>
        </action>
        <action>
            <action-name>add</action-name>
            <parameter>u</parameter>
            <parameter>variable A</parameter>
        </action>
        <action>
            <action-name>unmark</action-name>
            <parameter>u</parameter>
            <parameter>Waiting</parameter>
        </action>
        <action>
            <action-name>mark</action-name>
            <parameter>u</parameter>
            <parameter>Used</parameter>
        </action>
        <rule-comment/>
    </rule>
    <strategy>
        <strategy-name>CPLStrategy</strategy-name>
        <strategy-code>firstRule
  Stop
  NotNot
  And
  NotOr
  NotAnd
  Or
end</strategy-code>
        <strategy-comment/>
    </strategy>
    <strategy>
        <strategy-name>k4ConfluenceStrategy</strategy-name>
        <strategy-code>repeat
  firstRule
	CPLStrategy
	allRules
	  createSuccessor
	  propagateNec
	  createNewRoot
	  confluence
	  loopTest
	end
  end
end</strategy-code>
        <strategy-comment/>
    </strategy>
    <main-strategy>k4ConfluenceStrategy</main-strategy>
    <testing-formula>
        <formula-name/>
        <formula-code>and and ROOT pos P nec pos Q</formula-code>
        <formula-comment/>
    </testing-formula>
</logic>
